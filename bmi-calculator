# Calculate BMI
bmi = weight / (height ** 2)

# Print the result
print(f"Your BMI is: {bmi:.2f}")

# Categorize BMI
if bmi < 18.5:
    print("You are underweight.")
elif 18.5 <= bmi < 25:
    print("You have a normal weight.")
elif 25 <= bmi < 30:
    print("You are overweight.")
else:
    print("You are obese.")
